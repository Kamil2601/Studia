pom = copy.deepcopy(pom2)
        wieza = Figura()
        wieza = copy.deepcopy(pom2.bw)
        if r_wiezy(poz):
            
            wieza.x = 1
            if dozwolony_ruch(wieza,poz):
                print("WIEZA")
                pom.bw.x = 1
                kolejka.append(pom)
            wieza.x = 8
            if dozwolony_ruch(wieza, poz):
                print("WIEZA")
                pom.bw.x = 8
                kolejka.append(pom)
            wieza = copy.deepcopy(pom2.bw)
            wieza.y = 1
            if dozwolony_ruch(wieza, poz):
                print("WIEZA")
                pom.bw.y = 1
                kolejka.append(pom)
            wieza.y = 8
            if dozwolony_ruch(wieza, poz):
                print("WIEZA")
                pom.bw.y = 8
                kolejka.append(pom)
            
        else:
            a = poz.bk.x
            b = poz.bk.y
            for i in range(3):
                for j in range(3):
                    pom = copy.deepcopy(pom2)
                    pom.bk.x=a+j-1
                    pom.bk.y=b+i-1
                    if  dozwolona_pozycja(pom) and (not pom in lista) and (not pom in kolejka) and (i!=1 or j!=1):
                        kolejka.append(pom)



                    a = poz.bk.x
        b = poz.bk.y
        for i in range(3):
            for j in range(3):
                pom = copy.deepcopy(pom2)
                pom.bk.x=a+j-1
                pom.bk.y=b+i-1
