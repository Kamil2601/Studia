                pom.bw.y = poz.ck.y
                if not pom in kolejka:
                    kolejka.append(pom)


      
        else:#SZCZEGOLNE PRZYPADKI
            if ((poz.ck.x == 1 and poz.bk.x == 3) or  (poz.ck.x == 8 and poz.bk.x == 6))  and (poz.ck.y == poz.bk.y+1 or poz.ck.y == poz.bk.y-1):
                if poz.bk.x != poz.bw.x:
                    pom.bw.y = 1
                    if not pom in kolejka:
                        kolejka.append(pom)
                    pom.bw.y = 8
                    if not pom in kolejka:
                        kolejka.append(pom)
                else:
                    if (pom.bw.y==1 and pom.bk.y==2) or (pom.bw.y==8 and pom.bk.y==7):
                        pom.bk.y=pom.ck.y
                    elif pom.bw.y!=pom.bk.y+1 and pom.bw.y!=pom.bk.y-1:
                        pom.bw.x = 4
                    if not pom in kolejka:
                        kolejka.append(pom)
                    else:
                        if pom.bw.y<pom.bk.y:
                            pom.bw.y=1
                        else:
                            pom.bw.y=8
                        if not pom in kolejka:
                                kolejka.append(pom)
