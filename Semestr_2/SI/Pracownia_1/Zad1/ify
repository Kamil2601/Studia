 pom = copy.deepcopy(pom2)
        if poz.ck.x == 1 and poz.bk.x == 3 and poz.bw.y!=poz.ck.y-1 and poz.bw.y!=poz.ck.y and poz.bw.y!=poz.ck.y+1:
            pom.bw.x = 1
