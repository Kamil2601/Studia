                kolejka.append(pom)
        elif (poz.ck.x == 1 and poz.bk.x == 3) or (poz.ck.x == 8 and poz.bk.x == 6):
            if poz.ck.y == 1 and poz.bk.y == 2 and poz.bw.y > 2:
                pom.bw.x = poz.ck.x
                if not pom in kolejka:
                    kolejka.append(pom)
            elif poz.ck.y == 8 and poz.bk.y == 7 and poz.bw.y < 7:
                pom.bw.x = poz.ck.x
                if not pom in kolejka:
                    kolejka.append(pom)
        elif (poz.ck.y == 1 and poz.bk.y == 3) or (poz.ck.y == 8 and poz.bk.y == 6):
            if poz.ck.x == 1 and poz.bk.x == 2 and poz.bw.x > 2:
                pom.bw.y = poz.ck.y
                if not pom in kolejka:
                    kolejka.append(pom)
