        if poz.bw.x==poz.bk.x:
            if poz.bw.y>poz.bk.y:
                p = poz.bk.y+1
                k = 8
            else:
                p = 1
                k = poz.bk.y-1
        else:
            p = 1
            k = 8
        pom = copy.deepcopy(pom2)
        for i in range (p, k+1):
            pom.bw.y = i
            if (not pom in kolejka) and (not pom in lista):
                kolejka.append(pom)

