git init Repozytorium
cd Repozytorium
echo 'Kolejne czasy:' > log
git add log
git commit -m "Naglowek"
let R=$RANDOM%100;
for (( i=1; $i < $R; i++)); do
	date >> log
	git commit -a -m "Dodana data"
done
date > log
git commit -a -m "Dodana data"
let p=100-$R;
for (( i=1; $i < $p; i++)); do
	date >> log
	git commit -a -m "Dodana data"
done
